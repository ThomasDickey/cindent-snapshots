-T NCURSES_SP_ARGx
-T NCURSES_SP_DCLx
-T SCREEN
-T TERMTYPE
-T attr_t
-T cchar_t
-T chtype
-T NCURSES_COLOR_T
-T NCURSES_PAIRS_T

/* vile */
-T BUFFER
-T CHARTYPE
-T LINE
-T MARK
-T TBUFF
-T WINDOW

/* xterm */
-T ANSI
-T Bool
-T Boolean
-T CELL
-T Cardinal
-T Char
-T ColorRegister
-T Dimension
-T Display
-T Drawable
-T Graphic
